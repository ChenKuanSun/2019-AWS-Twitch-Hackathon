{
  "name": "twitch-extensions-live",
  "main": "main.js",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "ng build --base-href ./ && electron .",
    "build_win64": "ng build --base-href ./ && electron-packager . twitch-extensions-live-Win64 --out=release-builds --platform=win32 --arch=x64 --no-prune --ignore=/release-builds --ignore=/release --ignore=/node_modules --ignore=/release --ignore=/e2e --ignore=/src --overwrite --icon=./icon.png",
    "build_win32": "ng build --base-href ./ && electron-packager . twitch-extensions-live-Win32 --out=release-builds --platform=win32 --arch=ia32 --no-prune --ignore=/release-builds --ignore=/release --ignore=/node_modules --ignore=/release --ignore=/e2e --ignore=/src --overwrite --icon=./icon.png",
    "build_linux64": "ng build --base-href ./ && electron-packager . twitch-extensions-live-Linux64 --out=release-builds --platform=linux --arch=x64 --no-prune --ignore=/release-builds --ignore=/release --ignore=/node_modules --ignore=/release --ignore=/e2e --ignore=/src --overwrite --icon=./icon.png",
    "build_linuxArm": "ng build --base-href ./ && electron-packager . twitch-extensions-live-LinuxArm --out=release-builds --platform=linux --arch=armv7l --no-prune --ignore=/release-builds --ignore=/release --ignore=/node_modules --ignore=/release --ignore=/e2e --ignore=/src --overwrite --icon=./icon.png",
    "build_OSX64": "ng build --base-href ./ && electron-packager . twitch-extensions-live-OSX64 --out=release-builds --platform=darwin --arch=x64 --no-prune --ignore=/release-builds --ignore=/release --ignore=/node_modules --ignore=/release --ignore=/e2e --ignore=/src --overwrite",
    "build_all_platform": "ng build --base-href ./ && electron-packager . twitch-extensions-live-Win64 --out=release-builds --platform=win32 --arch=x64 --no-prune --ignore=/release-builds --ignore=/release --ignore=/node_modules --ignore=/release --ignore=/e2e --ignore=/src --overwrite --icon=./icon.png && electron-packager . twitch-extensions-live-Win32 --out=release-builds --platform=win32 --arch=ia32 --no-prune --ignore=/release-builds --ignore=/release --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --icon=./icon.png && electron-packager . twitch-extensions-live-Linux64 --out=release-builds --platform=linux --arch=x64 --no-prune --ignore=/release-builds --ignore=/release --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --icon=./icon.png && electron-packager . twitch-extensions-live-LinuxArm --out=release-builds --platform=linux --arch=armv7l --no-prune --ignore=/release-builds --ignore=/release --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --icon=./icon.png"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.13",
    "@angular/common": "~8.2.13",
    "@angular/compiler": "~8.2.13",
    "@angular/core": "~8.2.13",
    "@angular/forms": "~8.2.13",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/router": "~8.2.13",
    "@aws-amplify/ui": "^1.1.3",
    "@tensorflow-models/posenet": "^2.2.0",
    "@tensorflow/tfjs": "^1.3.2",
    "aws-amplify": "^1.2.4",
    "aws-amplify-angular": "^3.1.4",
    "aws-sdk": "^2.573.0",
    "electron": "^11.5.0",
    "electron-packager": "^14.1.0",
    "ngx-webcam": "^0.2.6",
    "rxjs": "~6.4.0",
    "subsink": "^1.0.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.18",
    "@angular/cli": "~8.3.18",
    "@angular/compiler-cli": "~8.2.13",
    "@angular/language-service": "~8.2.13",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
